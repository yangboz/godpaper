<?xml version="1.0" encoding="utf-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/3.6">
	<id>com.gamevil.zenonia2</id>
	<filename>GODPAPER ApplicationBase</filename>
	<name>GODPAPER ApplicationBase</name>
	<versionNumber>0.0.1</versionNumber>
	<description>GODPAPER example application built with Feathers UI controls for Starling</description>
	<copyright>2013 GODPAPER</copyright>

	<initialWindow>
		<content>build/deploy/ApplicationBase.swf</content>
		<autoOrients>true</autoOrients>
		<fullScreen>true</fullScreen>
		<visible>true</visible>
		<renderMode>direct</renderMode>
	</initialWindow>

	<supportedLanguages>en</supportedLanguages>

	<icon>
		<image29x29>assets/icons/icon_029.png</image29x29>
		<image48x48>assets/icons/icon_048.png</image48x48>
<!-- 		<image50x50>assets/icons/icon_048.png</image50x50> -->
		<image57x57>assets/icons/icon_057.png</image57x57>
<!-- 		<image58x58>assets/icons/icon_057.png</image58x58> -->
		<image72x72>assets/icons/icon_072.png</image72x72>
<!-- 		<image100x100>assets/icons/icon_128.png</image100x100> -->
<!-- 		<image114x114>assets/icons/icon_128.png</image114x114> -->
		<image128x128>assets/icons/icon_128.png</image128x128>
<!-- 		<image144x144>assets/icons/icon_128.png</image144x144> -->
	</icon>
	<android>
		<colorDepth>16bit</colorDepth>
		<manifestAdditions><![CDATA[
			<manifest android:installLocation="auto">
			    <uses-permission android:name="android.permission.INTERNET"/>
			</manifest>
		]]></manifestAdditions>
	</android>
	<iPhone>
		<InfoAdditions>
		<![CDATA[
			<key>UIDeviceFamily</key> 
			<array> 
				<string>1</string>
				<string>2</string>
			</array>
			<key>UIPrerenderedIcon</key>
			<true/>
		]]> 
		</InfoAdditions>
		<requestedDisplayResolution>high</requestedDisplayResolution>
	</iPhone>
</application>